public class Parabel extends Funktion{
  // Attribute
  double a; 
  double b; 
  double c;
// Konstruktor 
  public Parabel(double a, double b, double c) { 
    this.a = a; 
    this.b = b; 
    this.c = c; 
  }
// Implementierung der abstrakten Funktion 
  public double fktwert(double x) { 
    return a*x*x+b*x+c; 
  }

  // Anfang Attribute
  // Ende Attribute
  
  // Anfang Methoden
  // Ende Methoden
} // end of Parabel

